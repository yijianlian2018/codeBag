<view class="page"><van-search vue-id="1" value="{{value}}" placeholder="搜索" data-event-opts="{{[['^change',[['bindKeyInput']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-search><view class="container-inner"><view class="side-bar-letter-list touch-class"><view data-event-opts="{{[['tap',[['hotCity',['$event']]]]]}}" class="side-bar-hot-city" bindtap="__e"><view style="margin-top:0;">热</view></view><block wx:for="{{sideBarLetterList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view style="color:#35B1A7;font-size:20rpx;" data-letter="{{item.name}}" data-event-opts="{{[['touchend',[['touchSideBarLetter',['$event']]]]]}}" bindtouchend="__e">{{''+item.name+''}}</view></block></view><view class="container"><block wx:if="{{showChosenLetterToast}}"><block><view class="show-chosen-letter">{{toastShowLetter}}</view></block></block><scroll-view style="{{'height:'+(winHeight+'px')+';'}}" scroll-y="{{true}}" scroll-into-view="{{scrollTopId}}" scroll-top="{{scrollTop}}" data-event-opts="{{[['scroll',[['bindScroll',['$event']]]]]}}" bindscroll="__e"><view class="ul _ul"><block wx:for="{{completeList}}" wx:for-item="citys" wx:for-index="k" wx:key="k"><view class="li _li" data-city="{{citys.city}}" data-code="{{citys.code}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" bindtap="__e">{{citys.city}}</view></block></view><view class="city-picker"><view data-event-opts="{{[['tap',[['reGetLocation',['$event']]]]]}}" class="hotcity-common" bindtap="__e">点击定位城区</view><view class="current-city" data-city="{{city}}" data-code="{{currentCityCode}}" data-county="{{county}}" data-event-opts="{{[['tap',[['reGetLocation',['$event']]]]]}}" bindtap="__e">{{city+" "+county}}</view><view class="hotcity-common">热门城市</view><view class="hot-city"><block wx:for="{{hotCityList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view><view class="weui-grid" data-code="{{item.cityCode}}" data-city="{{item.city}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" bindtap="__e"><view class="weui-grid-label">{{item.city}}</view></view></view></block></view></view><block wx:for="{{cityList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="selection"><view class="item-letter" id="{{item.initial}}">{{item.initial}}</view><block wx:for="{{item.cityInfo}}" wx:for-item="ct" wx:for-index="k" wx:key="k"><view class="item-city" data-code="{{ct.code}}" data-city="{{ct.city}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" bindtap="__e">{{ct.city}}</view></block></view></block></scroll-view></view></view></view>