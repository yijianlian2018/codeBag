.class {
  padding: 20rpx 30rpx 50rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.class .thumbnail {
    width: 180rpx;
    height: 220rpx;
    margin-right: 20rpx;
}
.class .body {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
}
.class .body .title {
      font-weight: bold;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      display: -webkit-box;
      -webkit-box-orient: vertical;
      -webkit-line-clamp: 2;
}
.class .body .title .tag {
        width: 50rpx;
}
.class .body .sub {
      color: #999;
      font-size: 24rpx;
}
.box {
  padding: 30rpx;
}
.box .title {
    color: #999;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #f5f5f5;
}
.box .ewm-box {
    border: 1px solid #f5f5f5;
    -webkit-box-shadow: 0 10rpx 10rpx #f5f5f5;
            box-shadow: 0 10rpx 10rpx #f5f5f5;
}
.box .ewm-box .ewm-title {
      background-color: #f5f5f5;
      padding: 20rpx;
      font-size: 22rpx;
      color: #999;
      font-weight: bold;
}
.box .ewm-box .ewm-title .count {
        font-size: 26rpx;
        color: #333;
}
.box .ewm-box .tip {
      font-size: 22rpx;
      text-align: center;
      color: #999;
      margin-top: 20rpx;
}
.box .ewm-box .ewm {
      display: block;
      width: 300rpx;
      margin: auto;
      padding: 20rpx 0;
}
.box .ewm-box .num {
      text-align: center;
      font-size: 36rpx;
      color: #007bff;
      padding: 30rpx 0;
      font-weight: bold;
      border-top: 1px dashed #f5f5f5;
}
.btn {
  margin-top: 20rpx;
}
.navigate {
  padding: 20rpx 30rpx;
}
.navigate text {
    color: #999;
    font-size: 22rpx;
}
.box .info {
  padding-top: 20rpx;
  font-size: 28rpx;
  color: #555;
}
.box .info > view {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 5rpx 0;
}
.box .info .label {
  width: 200rpx;
  text-align: right;
}

