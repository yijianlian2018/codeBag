<view class="page"><van-tabs vue-id="1" type="card" color="#007BFF" animated sticky data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{tabs}}" wx:for-item="tab" wx:for-index="i" wx:key="i"><van-tab vue-id="{{('2-'+i)+','+('1')}}" title="{{tab.name}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{tab.list}}" wx:for-item="order" wx:for-index="k" wx:key="k"><view class="order"><view class="row"><view class="oid">{{"订单编号："+order.order_sn}}</view><block wx:if="{{order.order_status===0}}"><view class="status">未接单</view></block><block wx:else><block wx:if="{{order.order_status===1}}"><view class="status">已接单</view></block><block wx:else><view class="status">已完成</view></block></block></view><view class="row"><view>{{"项目名称："+order.project_name}}</view><view>{{"服务人："+order.serve_people_name}}</view></view><view class="row"><view>{{"项目金额：￥"+order.project_price}}</view><view>{{"资质："+order.qualifications}}</view></view><view class="row"><view class="time">{{"下单时间："+order.create_time}}</view><block wx:if="{{order.end_time}}"><view class="time">{{"完成时间："+order.end_time}}</view></block><block wx:else><view class="time">完成时间：未完成</view></block></view></view></block><block wx:if="{{tab.list.length===0&&!more}}"><image class="empty" src="/static/empty.png" mode="widthFix"></image></block><block wx:if="{{more}}"><view class="loading"><text>加载中</text><van-loading vue-id="{{('3-'+i)+','+('2-'+i)}}" type="spinner" color="#999" data-com-type="wx" bind:__l="__l"></van-loading></view></block></van-tab></block></van-tabs></view>