<view class="page"><block a:for="{{list}}" a:for-item="item" a:for-index="i" a:key="i"><van-cell vue-id="{{'1-'+i}}" size="large" center="{{true}}" title="{{item.user_name}}" onVueInit="__l"><block a:if="{{item.head_img}}"><image class="icon" slot="icon" src="{{item.head_img}}"></image></block><block a:else><image class="icon" slot="icon" src="../../../static/avatar.jpg"></image></block><navigator slot="right-icon" url="{{'/pages/message/chat/chat?name='+item.user_name+'&id='+item.id}}"><van-button vue-id="{{('2-'+i)+','+('1-'+i)}}" custom-class="btn" type="primary" size="small" onVueInit="__l">私信</van-button></navigator></van-cell></block><block a:if="{{more}}"><view class="loading"><text>加载中</text><van-loading vue-id="3" type="spinner" color="#999" onVueInit="__l"></van-loading></view></block></view>