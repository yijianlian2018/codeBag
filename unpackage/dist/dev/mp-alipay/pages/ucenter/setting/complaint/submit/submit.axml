<view class="page"><form data-event-opts="{{[['submit',[['onSubmit',['$event']]]]]}}" onSubmit="__e"><van-cell-group vue-id="1" onVueInit="__l"><van-field vue-id="{{('2')+','+('1')}}" name="Title" size="large" required clearable label="标题" placeholder="请填写投诉标题" onVueInit="__l"></van-field><van-field vue-id="{{('3')+','+('1')}}" name="Content" size="large" required clearable label="内容" type="textarea" placeholder="请填写投诉内容" autosize border="{{false}}" onVueInit="__l"></van-field><van-field vue-id="{{('4')+','+('1')}}" name="Mobile" size="large" clearable label="手机号" placeholder="请填写手机号" onVueInit="__l"></van-field><van-field vue-id="{{('5')+','+('1')}}" name="Email" size="large" clearable label="邮箱地址" placeholder="请填写邮箱地址" onVueInit="__l"></van-field></van-cell-group><button class="btn" formType="submit">提交投诉</button></form></view>