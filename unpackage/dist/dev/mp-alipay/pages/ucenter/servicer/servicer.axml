<view class="page"><view class="item"><block a:if="{{server.head_img}}"><image class="avatar" slot="icon" src="{{server.head_img}}"></image></block><block a:else><image class="avatar" slot="icon" src="../../../static/avatar.jpg"></image></block><view class="body"><view class="name">{{server.name}}</view><view class="desc">{{server.mobile}}</view></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="btn" onTap="__e">下单</view></view><uni-popup vue-id="1" show="{{show}}" maskClick="{{false}}" type="bottom" onVueInit="__l"><view class="close"><image src="../../../static/close.png" mode data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" onTap="__e"></image></view><form data-event-opts="{{[['submit',[['onSubmit',['$event']]]]]}}" onSubmit="__e"><input style="display:none;" name="server_people_id" type="text" data-event-opts="{{[['input',[['__set_model',['$0','id','$event',[]],['server']]]]]}}" value="{{server.id}}" onInput="__e"/><van-field vue-id="{{('2')+','+('1')}}" name="project_name" size="large" label="项目名称" title-width="180rpx" placeholder="请输入项目名称" clearable onVueInit="__l"></van-field><van-field vue-id="{{('3')+','+('1')}}" name="project_price" size="large" label="金额" title-width="180rpx" placeholder="请输入金额" clearable onVueInit="__l"></van-field><van-cell vue-id="{{('4')+','+('1')}}" size="large" title-width="180rpx" title="资质" onVueInit="__l"><input style="display:none;" name="IsZc" type="text" data-event-opts="{{[['input',[['__set_model',['','IsZc','$event',[]]]]]]}}" value="{{IsZc}}" onInput="__e"/><view class="check-box" slot="right-icon"><block a:for="{{list}}" a:for-item="item" a:for-index="i" a:key="i"><view data-event-opts="{{[['tap',[['checkType',[1]]]]]}}" class="check-item" onTap="__e"><text>{{item.qua_name}}</text><block a:if="{{type===1}}"><image class="check" src="../../../static/checked.png"></image></block><block a:else><image class="check" src="../../../static/unchecked.png"></image></block></view></block></view></van-cell><van-field vue-id="{{('5')+','+('1')}}" size="large" title-width="180rpx" label="可用代金券" value="请选择" input-align="right" readonly is-link onVueInit="__l"></van-field><button class="btn" formType="submit">确认下单</button></form></uni-popup></view>