<view class="page"><radio-group class="radio-group" name="{{item.QuestionId}}" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" onChange="__e"><block a:for="{{list}}" a:for-item="item" a:for-index="i" a:key="i"><label data-event-opts="{{[['tap',[['onClick',['$0'],[[['list','',i,'id']]]]]]]}}" class="radio" onTap="__e"><text>{{item.qua_name}}</text><radio class="icon" color="#007bff" value="{{item.qua_name}}"></radio></label></block></radio-group><button class="btn">保存</button></view>