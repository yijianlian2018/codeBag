<view class="page"><van-search vue-id="1" value="{{value}}" placeholder="搜索" onVueInit="__l"></van-search><van-tabs vue-id="2" animated color="#35B1A7" border="{{false}}" line-width="{{30}}" onVueInit="__l"><van-tab vue-id="{{('3')+','+('2')}}" title="国内" onVueInit="__l"><view class="container-inner"><view class="side-bar-letter-list touch-class"><view data-event-opts="{{[['tap',[['hotCity',['$event']]]]]}}" class="side-bar-hot-city" onTap="__e"><view style="margin-top:0;">热</view></view><block a:for="{{sideBarLetterList}}" a:for-item="item" a:for-index="index" a:key="index"><view style="color:#35B1A7;font-size:20rpx;" data-letter="{{item.name}}" catchtouchend="touchSideBarLetter">{{''+item.name+''}}</view></block></view><view class="container"><block a:if="{{showChosenLetterToast}}"><block><view class="show-chosen-letter">{{toastShowLetter}}</view></block></block><scroll-view style="{{'height:'+(winHeight+'px')+';'}}" scroll-y="{{true}}" scroll-into-view="{{scrollTopId}}" scroll-top="{{scrollTop}}" data-event-opts="{{[['scroll',[['bindScroll',['$event']]]]]}}" onScroll="__e"><view class="ul _ul"><block a:for="{{completeList}}" a:for-item="citys" a:for-index="k" a:key="k"><view class="li _li" data-city="{{citys.city}}" data-code="{{citys.code}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" onTap="__e">{{''+citys.city+''}}</view></block></view><view class="county-picker"><block a:if="{{showCountyPicker}}"><view class="county-picker-title">选择区县</view></block><block a:if="{{showCountyPicker}}"><view class="county"><block a:for="{{countyList}}" a:for-item="countys" a:for-index="j" a:key="j"><block class="county-picker-list"><view class="weui-grid" data-code="{{countys.id}}" data-city="{{countys.fullname}}" data-event-opts="{{[['tap',[['chooseCounty',['$event']]]]]}}" onTap="__e"><view class="weui-grid-label">{{countys.fullname}}</view></view></block></block></view></block></view><view class="city-picker"><view data-event-opts="{{[['tap',[['reGetLocation',['$event']]]]]}}" class="hotcity-common" onTap="__e">点击定位城区</view><view class="current-city" data-city="{{city}}" data-code="{{currentCityCode}}" data-county="{{county}}">{{city+" "+county}}</view><view class="hotcity-common">热门城市</view><view class="hot-city"><block a:for="{{hotCityList}}" a:for-item="item" a:for-index="i" a:key="i"><view><view class="weui-grid" data-code="{{item.cityCode}}" data-city="{{item.city}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" onTap="__e"><view class="weui-grid-label">{{item.city}}</view></view></view></block></view></view><block a:for="{{cityList}}" a:for-item="item" a:for-index="i" a:key="i"><view class="selection"><view class="item-letter" id="{{item.initial}}">{{item.initial}}</view><block a:for="{{item.cityInfo}}" a:for-item="ct" a:for-index="k" a:key="k"><view class="item-city" data-code="{{ct.code}}" data-city="{{ct.city}}" data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" onTap="__e">{{''+ct.city+''}}</view></block></view></block></scroll-view></view></view></van-tab><van-tab vue-id="{{('4')+','+('2')}}" title="国际" onVueInit="__l">内容 2</van-tab></van-tabs></view>